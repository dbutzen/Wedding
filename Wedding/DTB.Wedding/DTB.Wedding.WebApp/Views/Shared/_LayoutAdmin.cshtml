<!DOCTYPE html>
<html lang="en" style="background-color: #BFF5AC">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]Dan and Audrey Admin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body style="background-color: #BFF5AC">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <a class="navbar-brand" href="#">Program Declarations</a>
        <button class="navbar-toggler"
                type="button"
                data-toggle="collapse"
                data-target="#navbarNav"
                aria-controls="navbarNav"
                aria-expanded="false"
                aria-label="Toggle Navigation">
            <span class="animated-icon2"></span>
        </button>
        <div class="navbar-collapse collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" runat="server" href="~/Review">Review</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" runat="server" href="~/Family">Families</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" runat="server" href="~/Table">Tables</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" runat="server" href="~/Guest">Guests</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" runat="server" href="~/AdminLogout">Logout</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="container-fluid">
        <div class="row">
            

            <div class="col-md-9 container main body-content rounded-top">
                <div class="header rounded-top">
                    <h3>@ViewBag.Title</h3>
                </div>
                @RenderBody()
                <hr />
                <footer>
                    <p>&copy; @DateTime.Now.Year -Dan Butzen</p>
                </footer>
            </div>
        </div>
    </div>

</body>
</html>